{
  "name": "compiled-i18n",
  "version": "1.0.0",
  "description": "Framework-independent internationalization support for Vite-built projects",
  "homepage": "https://github.com/wmertens/compiled-i18n",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmertens/compiled-i18n.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wmertens/compiled-i18n/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "./dist/src.d.ts",
  "sideEffects": [
    "./dist/qwik.cjs",
    "./dist/qwik.mjs"
  ],
  "keywords": [
    "i18n",
    "rollup-plugin",
    "vite-plugin"
  ],
  "files": [
    "dist",
    "Readme.md",
    "qwik.md",
    "qwik",
    "vite"
  ],
  "author": "Wout Mertens <Wout.Mertens@gmail.com>",
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/src.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./qwik": {
      "types": "./dist/qwik.d.ts",
      "import": "./dist/qwik.mjs",
      "require": "./dist/qwik.cjs"
    }
  },
  "scripts": {
    "test": "vitest run --coverage",
    "test.watch": "vitest watch --color",
    "build": "vite build",
    "lint": "eslint .",
    "lint.fix": "eslint --fix .; prettier -w .",
    "dev": "vite",
    "release": "npm run build && npx np"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.5.1",
    "@types/babel__core": "^7.20.5",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitest/coverage-v8": "^1.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "vite": "^5.2.3",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.4.0"
  },
  "peerDependencies": {
    "@builder.io/qwik": "^1.2.18"
  },
  "peerDependenciesMeta": {
    "@builder.io/qwik": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/core": "^7.24.3",
    "@babel/plugin-syntax-typescript": "^7.24.1"
  }
}
