{
  "name": "vite-plugin-static-i18n",
  "version": "0.4.0",
  "description": "Framework-independent internationalization support for Vite-built projects",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "./dist/src.d.ts",
  "sideEffects": false,
  "keywords": [
    "i18n",
    "rollup-plugin",
    "vite-plugin"
  ],
  "files": [
    "dist",
    "Readme.md",
    "vite"
  ],
  "author": "Wout Mertens <Wout.Mertens@gmail.com>",
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/src.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    }
  },
  "scripts": {
    "test": "vitest run --coverage",
    "test.watch": "vitest watch --color",
    "build": "vite build",
    "lint": "eslint .",
    "lint.fix": "eslint --fix .; prettier -w .",
    "dev": "vite",
    "release": "npm run build && npx np"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^0.34.5",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsdoc": "^46.4.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vitest-globals": "^1.2.0",
    "prettier": "^3.0.0",
    "prettier-plugin-jsdoc": "^1.0.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.34.5"
  },
  "dependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-syntax-typescript": "^7.22.5"
  }
}
